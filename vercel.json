{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ],
  "excludeFiles": [
    "scrap.py",
    "scraper.py",
    "discourse_posts/**",
    "notebooks/**",
    "data/**",
    "test/**",
    "*.ipynb",
    "*.md",
    "*.pt",
    "*.onnx",
    "*.pdf",
    "*.csv",
    "*.xlsx",
    "__pycache__/**",
    ".env",
    "requirements-dev.txt"
  ]
}
